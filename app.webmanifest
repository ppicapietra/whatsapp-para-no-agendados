{
    "id": "com.ppicapietra.wanoa",
    "version": "1.6.7",
    "name": "WaNoA",
    "short_name": "WaNoA",
    "description": "Una herramienta para poder abrir chats de Whatsapp con números no agendados, desde la propia app o desde el menú compartir para el texto seleccionado del número de teléfono.",
    "category": ["productivity","utilities"],
    "lang": "es-AR",
    "dir": "ltr",
    "start_url": "index.html",
    "scope": "https://grupopietra.com/tools/wsp-ncs/",
    "display": "standalone",
    "orientation": "portrait",
    "background_color": "white",
    "theme_color": "#25D366",
    "screenshots": [{
        "src": "images/screenshot-1.jpg",
        "sizes": "1080x2034",
        "platform": "narrow",
        "label": "Abrir un chat en whatsapp indicando su teléfono"
      },
      {
        "src": "images/screenshot-2.jpg",
        "sizes": "1080x2034",
        "platform": "narrow",
        "label": "Abrir un chat desde el menú compartir, para el texto de un número de teléfono seleccionado"
      }],
    "share_target": {
        "method": "GET",
        "enctype": "application/x-www-form-urlencoded",
        "action": "https://grupopietra.com/tools/wsp-ncs/index.html",
        "title": "Enviar Whatsapp",
        "params": {
            "title": "title",
            "text": "text",
            "url": "url"
        }
    },
    "url_handlers": [
        {
            "origin": "https://grupopietra.com/tools/wsp-ncs/",
            "scheme": "web+wanoa",
            "protocol": "web+wanoa",
            "url": "index.html?q=%s",
            "title": "Abrir chat en Whatsapp",
            "icon": {
                "src": "images/whatsapp-16.png",
                "type": "image/png",
                "sizes": "16x16"
            }
        }
    ],
    "protocol_handlers": [
        {
            "protocol": "web+wanoa",
            "url": "index.html?q=%s"
        }
    ],
    "icons": [
        {
            "src": "images/whatsapp-512.png",
            "sizes": "512x512",
            "type": "image/png"
        },
        {
            "src": "images/whatsapp-256.png",
            "sizes": "256x256",
            "type": "image/png"
        },
        {
            "src": "images/whatsapp-192.png",
            "sizes": "192x192",
            "type": "image/png"
        },
        {
            "src": "images/whatsapp-128.png",
            "sizes": "128x128",
            "type": "image/png"
        },
        {
            "src": "images/whatsapp-16.png",
            "sizes": "16x16",
            "type": "image/png"
        }
    ]
}